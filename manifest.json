{
  "manifest_version": 2,
  "name": "TickTick in Sidebar",
  "description": "Displays TickTick in the sidebar.",
  "version": "1.0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "applications": {
    "gecko": {
      "strict_min_version": "73.0"
    }
  },
  "options_ui": {
    "page": "options/options.html",
    "browser_style": true
  },
  "sidebar_action": {
    "default_icon": {
      "16": "icons/16x16.png",
      "32": "icons/32x32.png"
    },
    "default_title": "TickTick",
    "default_panel": "sidebar/panel.html"
  },
  "icons": {
    "16": "icons/16x16.png",
    "32": "icons/32x32.png"
  },
  "commands": {
    "_execute_sidebar_action": {
      "suggested_key": {
        "default": "Alt+Shift+K"
      },
      "description": "Toggle the TickTick sidebar."
    }
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/16x16.png",
      "32": "icons/32x32.png"
    }
  }
}
